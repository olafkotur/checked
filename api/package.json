{
  "name": "api",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "build": "tsc",
    "server": "node ./build/index.js",
    "release": "heroku container:push web -a checked-api && heroku container:release web -a checked-api",
    "logs": "heroku logs -a checked-api --tail",
    "mock": "node mocker.js",
    "mongo:start": "mongod --dbpath db --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/mongodb": "^3.3.12",
    "body-parser": "^1.19.0",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "mongodb": "^3.3.5",
    "request": "^2.88.0",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
