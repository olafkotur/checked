{
  "name": "api",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "build": "tsc",
    "server": "node ./build/index.js",
    "release": "heroku container:push web -a checked-api && heroku container:release web -a checked-api",
    "logs": "heroku logs -a checked-api --tail",
    "mocker": "node mocker.js",
    "mongo:start": "mongod --dbpath db --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "test": "jest --verbose"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/mongodb": "^3.3.12",
    "@types/nodemailer": "^6.4.0",
    "base64-img": "^1.0.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.9.1",
    "jest": "^25.1.0",
    "moment": "^2.24.0",
    "mongodb": "^3.3.5",
    "nodemailer": "^6.4.2",
    "request": "^2.88.0",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
